@TECHREPORT{flynn:stripe-replicate-video-server,
author = {Robert Flynn and William Tetzlaff},
title = {Disk striping and block replication algorithms for video file 
servers},
institution = {IBM T. J. Watson Research Center},
number = {RC-20328},
year = 1996,
month = jan,
comment = {Using Zipf-like distribution function with a theta of 0.271,
about 14 percent of the movies account for half of the requests. They
randomly pick a block placement order permutation among the disks, and
choose to create non-overlapping mirrored copies of that 14 percent.
In this environment they expect to be able to support 380 simultaneous
1.5 Mbps streams on 24 Ruemmler-like disks with under 0.5 jitters per
hour per user. Without the mirroring of the high-demand material, it's
closer to 300. I'm not convinced their simulations are thorough enough
yet to rely on their numbers. This is for their Tiger Shark video
server. Discusses striping across less than the total number of disks,
which among other things reduces the vulnerability of the system to
disk failures. Recommends block size of 256KB.},
read = {1996/3/15}}

