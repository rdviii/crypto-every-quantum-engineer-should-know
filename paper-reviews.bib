@TechReport{floyd:short-term-file-ref,
  author = 	 {Rick Floyd},
  title = 	 {Short-Term File Reference Patterns in a {UNIX} Environment},
  institution =  {U. Rochester},
  year = 	 1986,
  number =	 {TR 177},
  month =	 mar,
  comment =      {Collected file system trace data on a 4.2BSD VAX in
                  a fairly heterogeneous environment (though I don't
                  think the VAX was serving files to anything else).
                  Tons of data, though they didn't keep exact records
                  on each read/write, and they didn't keep stat.
                  Since their work shows that most files are open only
                  for very short periods of time, they felt like that
                  justified their conclusions on read/write rate, but
                  I'm not so sure.  They did record the amount of data
                  read and written, just not the operation timestamps
                  and arguments.  Median size of files opened (as
                  opposed to simply existing in the FS) was 710
                  bytes.  Most of the data read, though, comes from
                  files larger than 20K.  68pct of files opened for
                  read are completely read, and 78pct of files opened
                  for write are completely written.  Half of all the
                  execute requests go to 13 executables.  They split
                  up the ops according to whether they were user,
                  system or net (e.g. UUCP and USENET) accessed.
                  84pct of the bytes moved were in reads.  20pct of
                  them were reads of /etc/passwd!  Useful data on how
                  skewed the activity is toward a few files.},
  location = { rdv : folder : local FS },
  read = { 1998/1/12 }
}

