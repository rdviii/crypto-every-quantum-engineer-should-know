@article{RIVEST1976371,
title = "On recognizing graph properties from adjacency matrices",
journal = "Theoretical Computer Science",
volume = "3",
number = "3",
pages = "371 - 384",
year = "1976",
issn = "0304-3975",
doi = "https://doi.org/10.1016/0304-3975(76)90053-0",
url = "http://www.sciencedirect.com/science/article/pii/0304397576900530",
author = "Ronald L. Rivest and Jean Vuillemin",
abstract = "A conjecture of Aanderaa and Rosenberg [15] motivates this work. We investigate the maximum number C(P) of arguments of P that must be tested in order to compute P, a Boolean function of d Boolean arguments. We present evidence for the general conjecture that C(P) = d whenever P(0d) ≠ P(1d) and P is invariant under a transitive permutation group acting on the arguments. A non-constructive argument (not based on the construction of an “oracle”) settles this question for d a prime power. We use this result to prove the Aanderaa-Rosenberg conjecture: at least v216 entries of the adjacency matrix of a v-vertex undirected graph G must be examined in the worst case to determine if G has any given non-trivial monotone graph property."
}

