@article{hoyer2003quantum,
  title={Quantum circuits with unbounded fan-out},
  author={H{\o}yer, P. and {\v{S}}palek, R.},
  journal={STACS 2003},
  pages={234--246},
  year={2003},
  publisher={Springer},
  comment = {Cites Siu for constant-depth arithmetic.  Cited in turn
                  by Browne and Kashefi.  Shows that fanout and
                  parity are equivalent, just surround with
                  Hadamards.  Uses constant-depth rotation by Hamming
                  weight, which requires constant-depth fanout.
                  Thm. 5 says add/mul/div/sort are in B-QNC^0_f.
                  Proof is very short, just cites Siu and says it's
                  easy to prove weighted threshold gate is in
                  B-QNC^0_f.
                  Okay, I think I understand this: B-QNC^0_f is
                  bounded-error, bounded-fanin, unbounded-fanout,
                  quantum Nick's class.  They show how a bounded-error
                  threshold gate can be created using the counting
                  gate, which requires approx QFT (I'll bet this hurts
                  precision), conditional rotations on all qubits
                  inverted using the fanout/fanin inversion technique,
                  then the inverse QFT.  Claim is that error is
                  one-sided, false-negative w/ Prob(1/n).  (How we got
                  from one-sided to two-sided error, I'm not sure.)
                  Also claims that exact threshold gate can be done in
                  O(log* n) time, which I probably do believe.
                  Paper ends w/ assertion that factoring is in
                  RP[B-QNC^0_f].  (Not sure what RP is for.)}
}

