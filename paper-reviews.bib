@Article{aspuru-guzik05:_simul_quant_comput_molec_energ,
  author = 	 {Al\'an Aspuru-Guzik and Anthony D. Dutoi and Peter
                  J. Love and Martin Head-Gordon},
  title = 	 {Simulated Quantum Computation of Molecular Energies},
  journal = 	 {Science},
  year = 	 2005,
  volume =	 309,
  pages =	 {1704--1707},
  comment = 	 {Uses phase estimation algorithms (PEA) to calculate
                  energy of e.g. the water molecule.  Shows three
                  types of basis sets, and three different mappings to
                  those, using 8 to 116 qubits for computations on
                  water.  The number of qubits grows linearly with the
                  number of basis vectors, and the number of gates
                  grows polynomially in that.  Has a very interesting
                  feature -- the calculations are done iteratively,
                  modifying the Hamiltonian used based on the previous
                  result, gaining a bit of precision in the final
                  answer for each iteration.  The output register only
                  has to be four qubits.  Since the modification is in
                  the function, rather than the input state, I don't
                  think this translates directly to e.g. factoring.
                  Says 30-100 qubits is enough to be useful.  Quantum
                  chemistry kinds of things.}
}

