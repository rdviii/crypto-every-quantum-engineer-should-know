@INPROCEEDINGS{van-meter:storage-media-pipelining,
crossref = {kobler:proc-third-nasa-msst},
author = {Van{ }Meter, Rodney},
title = {Storage Media Pipelining: Making Good Use of Fine-Grained Media},
pages = {303--311},
comment = {Fine-grained media are defined as those that can be read in
their entirety in a short period of time relative to their access time
-- for removable media, say, in an autochanger, one to two minutes
would be fine-grained. Access to such media can be made flexible and
fast by using a simple linear block layout, rather than striping, and
pipelining access for very long operations through the use of
extensive buffering in the host. The overall efficiency can be raised
from 50\% for striping to ~80\% for four-way pipelining for a specific
high-speed magneto-optical system.  Pipelining also offers more
flexibility than striping for both access patterns and hardware
configurations and could be considered more fault-tolerant.  The paper
is kind of weak, but the idea is original as far as I know.}}

