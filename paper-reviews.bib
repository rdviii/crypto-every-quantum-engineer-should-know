@article{levy11:qec-electronics,
  author={James E Levy and Malcolm S Carroll and Anand Ganti and Cynthia A Phillips and Andrew J Landahl and Thomas M
Gurrieri and Robert D Carr and Harold L Stalford and Erik Nielsen},
  title={Implications of electronics constraints for solid-state quantum error correction and quantum circuit failure probability},
  journal={New Journal of Physics},
  volume={13},
  number={8},
  pages={083021},
  url={http://stacks.iop.org/1367-2630/13/i=8/a=083021},
  year={2011},
  abstract={In this paper we present the impact of classical electronics constraints on a solid-state quantum dot logical qubit architecture. Constraints due to routing density, bandwidth allocation, signal timing and thermally aware placement of classical supporting electronics significantly affect the quantum error correction circuit's error rate (by a factor of ~3–4 in our specific analysis). We analyze one level of a quantum error correction circuit using nine data qubits in a Bacon–Shor code configured as a quantum memory. A hypothetical silicon double quantum dot quantum bit (qubit) is used as the fundamental element. A pessimistic estimate of the error probability of the quantum circuit is calculated using the total number of gates and idle time using a provably optimal schedule for the circuit operations obtained with an integer program methodology. The micro-architecture analysis provides insight about the different ways the electronics impact the circuit performance (e.g. extra idle time in the schedule), which can significantly limit the ultimate performance of any quantum circuit and therefore is a critical foundation for any future larger scale architecture analysis.}
}

