@article{acin2006epq,
  title={{Entanglement Percolation in Quantum Networks}},
  author={Ac\'in, A. and Cirac, J.I. and Lewenstein, M.},
  journal={Arxiv preprint quant-ph/0612167},
  year={2006},
  note={to appear in Nature Physics},
  comment={I don't care for this paper, at least not in its arXiv
                  form.  It seems to say that, in their approximation,
                  an infinitely long 1D chain of repeaters has no
                  chance of connecting unless the fidelity of all Bell
                  pairs is perfect.  Duh.  Oh, and their approximation
                  breaks down for any finite chain, which exhibits
                  some interesting properties (no kidding!) which they
                  then fail to detail.  The 2D lattices aren't much
                  better, and in fact, I'm not even sure I believe
                  their infinite approximation there.}
}

@Misc{acton:_near_perfect_meas,
  author =	 {M. Acton and K.-A. Brickman and P.C. Haljan and
                  P.J. Lee and L. Deslauriers and C. Monroe},
  title =	 {Near-Perfect Simultaneous Measurement of a Qubit
                  Register},
  howpublished = {http://arXiv.org/quant-ph/0511257},
  month =	 nov,
  year = 2005,
  comment = 	 {Good paper on measuring ions in an ion trap using an
                  intensified CCD.  They use the hyperfine states of
                  111Cd+ ions in a linear trap as the qubits.  A
                  photocathode and fluorescent screen are used to
                  intensify the photons coming from the ions
                  themselves, then focused onto a small CCD.  In their
                  example, a 4x4 group of pixels is grouped into a
                  single pixel, and a 7x7 block of those is used to
                  detect each ion.  Readout time is 15msec for the
                  whole CCD right now; they claim reducing that to
                  2.5usec/ion is doable with currently available
                  CCDs.  One limit in their current setup is the waist
                  of the detection beam is about 10 microns, compared
                  to an ion spacing of 4um.  They achieved readout
                  accuracy of 98\%, and a significant part of the
                  inaccuracy is believed to be due to neighboring ions
                  influencing each other.  Done on a chain of three
                  ions, the paper includes images of binary counting
                  from 000 to 111.}
}

