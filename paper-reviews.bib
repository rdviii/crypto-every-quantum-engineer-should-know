@Article{smith:file-mig,
  author = 	 {Alan Jay Smith},
  title = 	 {Long Term File Migration: Development and Evaluation
                  of Algorithms},
  journal = 	 cacm,
  year = 	 1981,
  volume =	 24,
  number =	 8,
  month =	 aug,
  pages =	 {521--532},
  comment =	 {Studies traces of file activity on mainframes,
                  looking for algorithms to choose which files to
                  migrate to tape.  His Stochopt uses the file size
                  and the recent access history to create the
                  metric.  Assumes variable-sized disk memory, so all
                  files that exceed the threshold are simply dumped,
                  rather than holding the smallest n files until the
                  disk is full.  entr, a simpler algorithm which just
                  uses the file size times the number of days since
                  last reference as the metric, works almost as well.
                  The actual curve fits aren't great, but generate
                  decent results.  Compares to both optimal algorithms
                  and such well-known ones as working set, assuming
                  files are all the same size (his works better --
                  that is, gives a higher cache hit rate).  Assumes
                  file retrieval times are all the same and queueing
                  effects are dealt with in only general terms, but
                  basic validity seems to hold.},
  location =	 { rdv : folder : HSM },
