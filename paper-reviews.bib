@Article{petta05:_coher_manip,
  author = 	 {J. R. Petta and A. C. Johnson and J. M. Taylor and
                  E. A. Laird and A. Yacoby and M. D. Lukin and
                  C. M. Marcus and M. P. Hanson and A. C. Gossard},
  title = 	 {Coherent manipulation of coupled electron spins in
                  semiconductor quantum dots},
  journal = 	 {Science},
  year = 	 2005,
  volume =	 309,
  pages =	 {2180--2184},
  comment = 	 {Excellent paper on a pair of dots used as a spin
                  qubit.  They move the electrons back and forth
                  between the dots to average out the effects of
                  nearby GaAs nuclei, which cause decoherence.  If I
                  have this right, they prepare the (0,2) singlet
                  (S = 0) state (both in R dot, |ud> + |du>?).  They
                  then separate (based on spin) the two electrons,
                  evolve, then project back onto the (0,2)S state to
                  measure.  "Native" decoherence time T2* is ~10nsec.
                  Adding the swapping back and forth, they get
                  >1usec.  Measurement is measurement of the
                  conductance of the R dot, and requires an additional
                  structure.  For these experiments, prep time is
                  200nsec, the electrons are separated for ~200nsec,
                  then brought back together for measurement for
                  5-10usec (a long time!).  The amount of time they
                  can be separated depends on magnetic field; with
                  zero field, drops off to 0.5 prob. singlet
                  measurement after 20nsec; for 100mT, 0.7, after
                  which it saturates.  Says their approach is
                  effectively a DFS.  Says all prep, protecting and
                  measurement can be done by local electrostatic
                  control.  I think this means gates require local
                  magnetic fields???}
}

@article{Pfaff29052014,
