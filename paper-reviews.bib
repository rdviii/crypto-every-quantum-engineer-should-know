@article{1367-2630-8-8-129,
  author={M Hayashi and K Iwama and H Nishimura and R Raymond and S Yamashita},
  title={(4,1)-Quantum random access coding does not exist: One qubit is not enough to recover one of four bits},
  journal={New Journal of Physics},
  volume={8},
