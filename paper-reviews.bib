@article{maity23:sigmetrics,
author = {Maity, Ananda G. and Casapao, Joshua C. A. and Benchasattabuse, Naphan and Hajdusek, Michal and Van Meter, Rodney and Elkouss, David},
title = {Noise estimation in an entanglement distillation protocol},
year = {2023},
