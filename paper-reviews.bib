@article{1367-2630-17-9-093008,
  author={Amir Kalev and Itay Hen},
  title={Fidelity-optimized quantum state estimation},
  journal={New Journal of Physics},
  volume={17},
  number={9},
  pages={093008},
  url={http://stacks.iop.org/1367-2630/17/i=9/a=093008},
  year={2015},
  abstract={We describe an optimized, self-correcting procedure for the Bayesian inference of pure quantum states. By analyzing the history of measurement outcomes at each step, the procedure returns the most likely pure state, as well as the optimal basis for the measurement that is to follow. The latter is chosen to maximize, on average, the fidelity of the most likely state after the measurement. We also consider a practical variant of this protocol, where the available measurement bases are restricted to certain limited sets of bases. We demonstrate the success of our method by considering in detail the single-qubit and two-qubit cases, and comparing the performance of our method against other existing methods.}
}

