@inproceedings{Almeida96,
  author    = "Virgilio Almeida and Azer Bestavros and Mark Crovella 
          and Adriana de Oliveira",
  title     = "Characterizing Reference Locality in the {WWW}",
  organization = "IEEE",
  booktitle = "IEEE PDIS'96: The International Conference in Parallel
             and Distributed Information Systems",
  month     = dec,
  address   = "Miami Beach, Florida",
  publisher = "",
  pages     = "",
  year      = "1996",
  annotate  = "http://cs-www.bu.edu/faculty/best/res/papers/pdis96.ps
    This paper tries to proof that server traces have temporal as well as
    spatial locality. Temporal locality is measured by marginal distribution
    which is the likelihood of a certain inter-reference distance. Spatial
    locality is measured by correlation structure which is the likelihood
    that future reference can be predicted based on a stream of past
    references. They show that a popularity profile abstracts
    over locality properties: scrambling a trace preserves its popularity
    profile while changing its locality properties. Their proof of existence
    of temporal locality is that a scrambled trace has a higher mean (LRU) stac
k
    distance than an unscrambled one. The existence proof for spatial locality
    is similar but considering the number of unique sequences instead of stack
    distance. According to their data a client trace has a higher spatial
    locality than a server trace than a scrambled server trace. Temporal 
    locality is characterized as a lognormal distribution. Spatial locality
    is modelled as a fractal of significantly > 0.5-self-similarity 
    (0.5-self-similarity has no long-range dependencies = Gaussian noise).
    Thus, they show that the correlation structure of server traces bear
    long range dependencies. Furthermore, they show how to generate
    synthetic traces based on their findings."
}

@InProceedings{chang99,
  author =       "Fay Chang and Garth A. Gibson",
  title =        "Automatic {I}/{O} Hint Generation through Speculative
                 Execution",
  booktitle =    "Operating Systems Design and Implementation ({OSDI}
                 '99)",
  year =         "1999",
  pages =        "1--14",
  annote =       "While the actual process is blocked by reads, a
                 speculative thread runs ahead, and generates read hints
                 (used for prefetching) instead of reads (and, of
                 course, it does not produce output). If the speculative
                 thread appears to stray from the normal execution path
                 or fall behind, it is reinitialized from the current
                 state of the main thread. This idea has been tested on
                 three read-intensive programs and produced 29\%--70\%
                 speedup on a machine with a 4-disk RAID~0 (fewer disks
                 produce less speedup, more disks hardly more). If the
                 hints were ignored, the slowdown was 1\%--4\%,
                 indicating that the scheme costs little if it is
                 useless; however, the Gnuld benchmark had a 15\%
                 slowdown when using the hints with one disk, apparently
                 because scarce disk bandwidth is wasted on useless
                 prefetches. For two of the benchmarks the hints are
                 quite accurate, but for Gnuld many hints caused
                 prefetches of useless blocks, and useful blocks were
                 not prefetched; Gnuld was also the only benchmark where
                 manual prefetching seriously outperformed the new
                 technique.",
}

