@article{altepeter2005photonic,
  title={Photonic state tomography},
  author={Altepeter, Joseph B and Jeffrey, Evan R and Kwiat, Paul G},
  journal={Advances in Atomic, Molecular, and Optical Physics},
  volume={52},
  pages={105--159},
  year={2005},
  publisher={Elsevier},
  comment = {Notes recommended by Stephen Crain (Jungsang's group) for
                  one- and two-qubit tomography.  Also describes
                  quantum tangle and concurrence.  In the description
                  of reconstructing d.m.s from data, it mentions that
                  the Stokes parameters alone can give you an illegal
                  d.m.!  They actually use a Maximum Likelihood
                  algorithm for the reconstruction.  Focus is on
                  optical state detection, and includes systemic
                  problems such as crosstalk and imperfect beam
                  splitters and "accidental counts" (two-photon, or
                  two-pair, emission and detection events).  If you
                  have only $n$ detectors, you can detect e.g. |0> but
                  not |1>, so the others are treated as loss events.
                  With $2n$ detectors, you can detect both states in
                  one shot, allowing ultimately $3^n$ settings instead
                  of $4^n$, e.g. 9 v. 15 for 2-qubit states.  (This 9
                  comes from 4 detectors, so 36 total results, or 9
                  discriminations of the 4 possible states in a
                  two-qubit measurement.)  The other 6 Stokes
                  parameters are linearly dependent on these same
                  results.}
}

