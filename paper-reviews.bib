  author =       "Udi Manber and Sun Wu",
  title =        "{GLIMPSE}: {A} Tool to Search Through Entire File
                 Systems",
  booktitle =    "Proceedings of USENIX Winter Technical Conference",
  publisher =    "USENIX",
  address =      "Berkeley, CA, USA",
  year =         "1994",
  ISBN =         "1-880446-58-8",
  institution =  "Department of Computer Science, University of
                 Arizona",
  pages =        "23--32",
  month =        "Winter",
  bibdate =      "Wed Aug 13 10:48:45 MDT 1997",
  abstract =     "Inverted indexes give fast searches for AND, OR
                 queries but indexes are large, typically 50\% to 300\%
                 of original file size. Glimpse trades some time to save
                 space; its indexes are typically 2\% to 4\% of
                 collection size. Uses 2-level-searching: index provides
                 pointers to *area* where info' may be found, not
                 precise location. The area is then searched
                 sequentially. Uses agrep to search both the index and
                 the local files. This allows approx' matching and
                 regular expressions. http://glimpse.cs.arizona.edu/
                 ([glimpse.cs.arizona.edu])[9/97]",
  acknowledgement = ack-nhfb,
}


